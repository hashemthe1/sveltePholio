generator client {
  provider = "prisma-client-js"
}

datasource db {
  // provider = "sqlite"
  // url      = env("DEV_DATABASE_URL")
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Contact {
  id          Int    @id @default(autoincrement())
  appName     String
  contactName String
  email       String
  message     String
}
